{
  "name": "@exodb/core",
  "version": "0.0.0",
  "description": "ExoDB is a client side database that provides an easy mongob-like interface to the data with first class typescript support. Data persistance can be archieved by using storage providers which save the data through a JSON interface to placed such as localStorage.",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint src/**.ts --max-warnings=0",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "build": "rimraf ./dist && tsc",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxnowack/exodb.git"
  },
  "keywords": [
    "database",
    "local-database",
    "client-database",
    "client",
    "offline-first",
    "rxdb",
    "typescript",
    "reactive",
    "meteor",
    "minimongo",
    "tracker",
    "mongodb",
    "replication",
    "synchronization"
  ],
  "author": "Max Nowack <max.nowack@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@maverick-js/signals": "^5.11.2",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "jest": "^29.5.0",
    "jest-github-actions-reporter": "^1.0.3",
    "meteor-ts-tracker": "^2.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "fast-sort": "^3.4.0",
    "mingo": "^6.4.3"
  }
}
